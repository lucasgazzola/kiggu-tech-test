  "description": "Backend Node.js con Express y TypeScript para prueba t√©cnica.",
  "main": "dist/src/server.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "prisma:resetdb": "npx prisma migrate reset --force && npx prisma migrate dev --name init",
    "test:inBand": "cross-env NODE_ENV=test jest --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand",
    "azure:deploy": "npm install && npm run build && npm start dist/src/server.js"
  },
